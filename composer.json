{
    "name": "innmind/time-warp",
    "type": "library",
    "description": "Time warp",
    "keywords": ["time", "warp"],
    "homepage": "http://github.com/Innmind/TimeWarp",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/TimeWarp/issues"
    },
    "require": {
        "php": "~8.0",
        "innmind/time-continuum": "~3.0",
        "psr/log": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\TimeWarp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\TimeWarp\\": "tests/",
            "Fixture\\Innmind\\TimeWarp\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.1",
        "innmind/coding-standard": "^1.0"
    }
}
